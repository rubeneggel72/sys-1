      <div class="row my-4">
        <div class="col-lg-12 col-md-12 mb-md-0 mb-4">
          <div class="card">
            <div class="card-header pb-0">
              <div class="row">
                <div class="col-lg-6 col-7">
                  <h6>Visualizacion registro: Persona</h6>
                 
                </div>

               
                

                
                <div class="col-lg-6 ">
                
                  <div class="row">
                    <div class="col">
             
                    </div>
                    <div class="col text-end text-xs font-weight-bolder  text-black-100">
                      <b>Creado <%= organizacion.createdAtSH %> </b>-
                      
                      <b>Actualizado <%= organizacion.updatedAtSH %> </b>
                     
                    </div>
                  </div>
                </div>                
                
                <div class="row form-group mb-4">
                                      <div class="col">
                    <label for="firstName">ID:</label>
                    <input type="text" class="form-control" id="firstName" name="firstName" value="<%= organizacion.idorganizacion %>" placeholder="id" disabled>
                  </div>
                </div>
                <div class="row form-group mb-4">
                  <div class="col">
                    <label for="razonSocial">Razon Social</label>
                    <input type="text" class="form-control" id="razonSocial" name="razonSocial" value="<%=organizacion.razonSocial %>" placeholder="Razon Social" >
                  </div>
                  <div class="col">
                    <label for="nombre">Nombre Fantasía</label>
                    <input type="text" class="form-control" id="nombre" name="nombre" value="<%=organizacion.nombre %>" placeholder="Nombre Fantasía" >
                  </div>

              <div class="row form-group mb-4">
                <div class="col">
                  <label for="direccionAdministracion">Direccion Administracion</label>
                  <input type="text" class="form-control" id="direccionAdministracion" name="direccionAdministracion" value="<%=organizacion.direccionAdministracion %>" placeholder="Direccion" required>
                </div>
                <div class="col">
                  <label for="ciudadAdministracion">Ciudad Administracion</label>
                  <input type="text" class="form-control" id="ciudadAdministracion" name="ciudadAdministracion" value="<%=organizacion.ciudadAdministracion %>" placeholder="Ciudad" required>
                </div>
              </div>

              <div class="row form-group mb-4">
                <div class="col">
                  <label for="tel">Provincia Administracion</label>
                  <input type="text" class="form-control" id="provinciaAdministracion" name="provinciaAdministracion" value="<%=organizacion.provinciaAdministracion %>" placeholder="Provincia" >
                </div>
            
                <div class="col">
                  <label for="text">Pais Administracion</label>
                  <input type="text" class="form-control" id="paisAdministracion" name="paisAdministracion" value="<%=organizacion.paisAdministracion %>" placeholder="Email" >
                </div>

              <BR>
                
              <div class="row form-group mb-4">
                <div class="col">
                  <label for="cui">Direccion Planta Ind.</label>
                  <input type="text" class="form-control" id="direccionPlantaIndustrial" name="direccionPlantaIndustrial" value="<%=organizacion.direccionPlantaIndustrial %>" placeholder="Direccion">
                </div>
              
              <div class="col">
                <label for="dni">Ciudad Planta Industrial</label>
                <input type="text" class="form-control" id="ciudadPlantaIndustrial" name="ciudadPlantaIndustrial" value="<%=organizacion.ciudadPlantaIndustrial %>" placeholder="Ciudad" >
              </div>
            </div>

            <div class="row form-group mb-4">            
              <div class="col">
                <label for="cui">Provincia Planta Industrial</label>
                <input type="text" class="form-control" id="provinciaPlantaIndustrial" name="provinciaPlantaIndustrial" value="<%=organizacion.provinciaPlantaIndustrial %>" placeholder="Provincia">
              </div>
            
            <div class="col">
              <label for="dni">Pais Planta Ind.</label>
              <input type="text" class="form-control" id="paisPlantaIndustrial" name="paisPlantaIndustrial" value="<%=organizacion.paisPlantaIndustrial %>" placeholder="Pais" >
            </div>
          </div>

          <div class="row form-group mb-4">           
            <div class="col">
              <label for="cui">Personas</label>
              <input type="text" class="form-control" id="personas" name="personas" value="<%=organizacion.personas %>" placeholder="personas">
            </div>
         
        </div>

        <div class="row form-group mb-4">    
      
          <div class="col">
          
                  <label for="details">Detalle</label>
                  <textarea class="form-control" name="details" id="details" cols="30" rows="12" placeholder="cliente Details"><%=organizacion.personas %></textarea>
              </div>
              
                <div class="form-group mb-4">
                  
                  <a href="/organizacion/index" class="btn btn-info" role="button">Volver</a>
                 
                </div>
   



      <%- include ('../partials/_footer.ejs')  %> 



    </div>
  </main>
